{
  "version": 2,
  "builds": [
    {
      "src": "api/python/stats.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "installCommand": "pip install --target ${LAMBDA_TASK_ROOT} -r requirements.txt"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/python/stats",
      "dest": "/api/python/stats.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/python/stats.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
