{
  "version": 2,
  "functions": {
    "api/python/**/*.py": {
      "runtime": "python3.12"
    }
  },
  "builds": [
    {
      "src": "api/python/stats.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/python/stats",
      "dest": "api/python/stats.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
